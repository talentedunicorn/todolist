{
  "name": "todone",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@now/node": "^1.6.1",
    "@types/file-saver": "^2.0.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.2",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "axios": "^0.20.0",
    "file-saver": "^2.0.2",
    "localforage": "^1.7.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-ga": "^3.0.0",
    "react-markdown": "^5.0.2",
    "react-scripts": "3.4.4",
    "remark-gfm": "^1.0.0",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom-fourteen",
    "test:coverage": "npm run test -- --coverage --watchAll=false",
    "test:watch": "npm run test --watch",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.8.0",
    "@testing-library/react": "^11.1.0",
    "codecov": "^3.7.1",
    "husky": "^4.3.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "coverageReporters": [
      "html",
      "lcov",
      "text"
    ],
    "collectCoverageFrom": [
      "src/**/*.(ts|tsx)",
      "!src/index.tsx",
      "!src/react-app-env.d.ts",
      "!src/models/**.ts"
    ]
  }
}
